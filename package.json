{
  "name": "coli",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint:scripts": "eslint . --fix",
    "lint:styles": "stylelint \"**/*.{vue,scss,sass,css}\" --fix --allow-empty-input",
    "svg:generate-sprite": "tsx scripts/generate-sprite.ts",
    "prettify": "prettier . --write --log-level silent --ignore-unknown",
    "vite-dev": "vite --host",
    "vite-build": "vite build",
    "vite-preview": "vite preview --host",
    "lighthouse": "unlighthouse --site http://192.168.1.145:4173/ --no-cache --desktop",
    "vue-tsc-check": "vue-tsc --build --force",
    "vue-tsc-watch": "vue-tsc --build --watch --preserveWatchOutput --pretty",
    "max-old-space": "cross-env NODE_OPTIONS=--max-old-space-size=2048",
    "dev": "vite",
    "preview": "npm-run-all --parallel vite-preview lighthouse",
    "type-check": "npm-run-all vue-tsc-check",
    "build-only": "npm-run-all vite-build",
    "build-local": "npm-run-all type-check vite-build",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=2048 npm run svg:generate-sprite && cross-env NODE_OPTIONS=--max-old-space-size=2048 vue-tsc --noEmit && cross-env NODE_OPTIONS=--max-old-space-size=2048 npm run vite-build",
    "lint": "npm-run-all --parallel lint:scripts lint:styles",
    "pre-commit": "npm-run-all lint prettify",
    "postinstall": "husky"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.12",
    "@floating-ui/vue": "^1.1.5",
    "@unhead/vue": "^1.11.11",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^8.8.1",
    "@vueuse/core": "^11.2.0",
    "@vueuse/integrations": "^11.2.0",
    "@vueuse/router": "^11.2.0",
    "@vueuse/shared": "^11.2.0",
    "axios": "^1.7.7",
    "bowser": "^2.11.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "element-plus": "^2.6.0",
    "fast-equals": "^5.0.1",
    "fuse.js": "^7.0.0",
    "lodash": "^4.17.21",
    "maska": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "p-limit": "^6.1.0",
    "pinia": "^2.2.6",
    "simplebar-vue": "^2.3.5",
    "unhead": "^1.11.11",
    "universal-cookie": "^7.2.2",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-sonarjs": "^2.0.4",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss-html": "^1.8.0",
    "prettier": "^3.7.3",
    "sass": "^1.94.2",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^3.1.5"
  },
  "lint-staged": {
    "*": "npm run prettify",
    "*.{ts,tsx,vue,js,jsx,cjs,mjs}": [
      "npm run lint:scripts",
      "bash -c 'npm run vue-tsc-check'"
    ],
    "*.{vue,scss,sass,css}": "npm run lint:styles"
  }
}
